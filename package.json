    "mikro-orm",
    "pglite",
    "postgresql",
    "postgres"
  ],
  "type": "module",
  "version": "1.0.3",
  "description": "A mikro-orm driver for pglite",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
